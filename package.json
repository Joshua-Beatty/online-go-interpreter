  "name": "online-go-interpreter",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/Joshua-Beatty/online-go-interpreter#readme",
  "bugs": {
    "url": "https://github.com/Joshua-Beatty/online-go-interpreter/issues"
