  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joshua-Beatty/online-go-interpreter.git"
  },
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "run-p build-go copy-wasm_exec",
    "build-go": "cd go-runner && cross-env GOOS=js GOARCH=wasm go build -o ../src/build/main.wasm",
    "postbuild-go": "node scripts/split-wasm.js && rimraf src/build/main.wasm",
    "copy-wasm_exec": "node scripts/copy-wasm_exec.js",
    "serve": "cd src && http-server",
    "dev": "npm run build && npm run serve"
  },
  "dependencies": {
    "cross-env": "^10.1.0",
    "http-server": "^14.1.1",
    "mkdirp": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "shx": "^0.4.0"
  }
}
